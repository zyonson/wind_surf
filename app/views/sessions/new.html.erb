<h1>ログイン</h1>
<%= form_for(:session, url: login_path) do |f| %>
  <div>
    <%= render 'shared/success_messages' %>
    <% if flash[:danger] %>
      <%= flash[:danger] %>
    <% end %>
  </div>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit "Log in" %>
<% end %>
